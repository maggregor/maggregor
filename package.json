    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.2",
